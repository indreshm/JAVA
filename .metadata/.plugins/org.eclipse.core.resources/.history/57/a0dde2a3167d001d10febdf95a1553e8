package vechile;
class Car extends Vehicle{
    void print(){
        System.out.print("Car class");
    }

    public static void main(String[] args){   
        Car obj1 = new Car();
        obj1.print();
        Vehicle obj2 = new Vehicle();
        obj2.print();  
    }
}
