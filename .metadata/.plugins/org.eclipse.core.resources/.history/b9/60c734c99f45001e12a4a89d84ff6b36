import java.util.Scanner;
public class arrayUse {
	public static void main(String[] args) {
        int[] arr= new int[10];
        arr[3]= 15;
        System .out.println(arr[3]);
        arr[10]= 15;
        arr[-1]= 15;
       // System.out.println(arr[10]);
     //   System.out.println(arr[-1]);
        System.out.println(arr[8]);
	}

}
